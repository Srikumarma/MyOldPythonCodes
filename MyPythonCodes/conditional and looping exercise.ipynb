   "source": [
    "#calculator using switch\n",
    "x=10\n",
    "y=5"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "3e5c3427",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter choice[1/2/3/4]: 4\n",
      "[1] \"5 / 10 = 0.5\"\n"
     ]
    }
   ],
   "source": [
    "choice = as.integer(readline(\"Enter choice[1/2/3/4]: \"))\n",
    "num1 = 5\n",
    "num2 = 10\n",
    "operator = switch(choice,\"+\",\"-\",\"*\",\"/\")\n",
    "result = switch(choice, num1+num2, num1-num2, num1*num2, num1/num2)\n",
    "print(paste(num1, operator, num2, \"=\", result))"
   ]
  },
