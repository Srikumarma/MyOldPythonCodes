    "import schedule\n",
    "import time\n",
    "def do_noth():\n",
    "    f1=open('new','w+')\n",
    "    f1.write('hi hello')\n",
    "    f1.close()\n",
    "    f1=open('new','r')\n",
    "    print(f1.read())\n",
    "    \n",
    "schedule.every(10).seconds.do(do_noth)\n",
    "\n",
    "while 1:\n",
    "    schedule.run_pending()\n",
    "    time.sleep(1)"
